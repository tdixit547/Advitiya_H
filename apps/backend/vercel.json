{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ],
    "crons": [
        {
            "path": "/api/keep-alive",
            "schedule": "0 0 * * *"
        }
    ]
}