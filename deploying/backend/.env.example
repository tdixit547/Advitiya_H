# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/smart-link-hub

# Redis Connection
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Server Configuration
PORT=3001
NODE_ENV=development

# Cache Settings
